{
    "name": "resumex/sdk",
    "description": "Official PHP SDK for ResumeX API - AI-powered CV generation platform",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ResumeX Team",
            "email": "tech@resumex.com"
        }
    ],
    "keywords": [
        "resumex",
        "cv",
        "resume",
        "api",
        "sdk",
        "laravel"
    ],
    "homepage": "https://github.com/resumex/sdk-php",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "ResumeX\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ResumeX\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ResumeX\\ResumeXServiceProvider"
            ],
            "aliases": {
                "ResumeX": "ResumeX\\Facades\\ResumeX"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
